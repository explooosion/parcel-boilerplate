{
  "name": "parcel-boilerplate",
  "version": "1.0.0",
  "description": "A simple project boilerplate with Parcel web app bundler",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html --open",
    "start:3000": "parcel -p 3000 ./src/index.html --open",
    "watch": "parcel watch ./src/index.html",
    "build": "parcel build ./src/index.html -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alekpentchev/parcel-boilerplate.git"
  },
  "keywords": [
    "webapp",
    "parcel",
    "js",
    "css",
    "html"
  ],
  "author": "Aleksander Pentchev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/alekpentchev/parcel-boilerplate/issues"
  },
  "homepage": "https://github.com/alekpentchev/parcel-boilerplate#readme",
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.4.1"
  },
  "dependencies": {}
}
